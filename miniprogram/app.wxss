/**app.wxss**/
@import './style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  justify-content: space-around;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}

page {
  background: #191919;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}

